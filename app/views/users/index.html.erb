<fieldset>
  <legend> register </legend>
<form action="/users" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label for="first Name">First Name:</label>
  <input type="text" name="user[First_name]" id="Name">
  <label for="last name">Last Name:</label>
  <input type="text" name="user[Last_name]" id="Last_name">
  <label for='password'>email</label>
  <input type="text" name="user[email]" id="email">
  <label for="city">City:</label>
  <input type="text" name="user[city]">
   <select name="user[state]" id="state">
  <% @states.each do |state| %>
    <option value="<%= state %>">
    <%= state %>
    <% end %>
  </select>
  <label for='password'>password</label>
  <input type="text" name="user[password]" id="password">
  <label for="password_conf"> Password Confirmation </label>
  <input type="text" name="user[password_confirmation]">
  <input type="submit" value="Create">
</form>
</fieldset>
<fieldset>
  <legend> login </legend>
<form action="/login" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <label for="email">email</label>
  <input type="text" name="email" id="email">
  <label for='password'>password</label>
  <input type="text" name="password" id="password">
  <input type="submit" value="Create">
</form>
</fieldset>
<% if flash[:error] %>
 <%= flash[:error] %>
 <% end %>
